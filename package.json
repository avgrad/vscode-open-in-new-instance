{
	"name": "open-in-new-instance",
	"displayName": "Open in new Instance",
	"description": "This extension allows you to open a folder from the VSCode Explorer in a new Instance of VSCode from the context menu.",
	"publisher": "sydeslyde",
	"repository": {
		"type": "git",
		"url": "https://github.com/sydeslyde/vscode-open-in-new-instance.git"
	},
	"homepage": "https://github.com/sydeslyde/vscode-open-in-new-instance/blob/master/README.md",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openInNewInstance"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openInNewInstance",
				"title": "Open in new Instance"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "extension.openInNewInstance",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "extension.openInNewInstance",
					"when": "explorerResourceIsFolder",
					"group": "2_workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"pack": "vsce package"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.61.0",
		"vscode": "^1.1.28"
	}
}
